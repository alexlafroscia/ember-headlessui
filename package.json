{
  "name": "ember-headlessui",
  "version": "0.9.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/GavinJoyce/ember-headlessui.git"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:other": "prettier --check \"**/*.{json,md,yml}\"",
    "lint:other:fix": "prettier --write \"**/*.{json,md,yml}\"",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "release": "dotenv release-it --",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ember/render-modifiers": "^1.0.2",
    "@embroider/macros": "^0.43.5",
    "ember-cli-babel": "^7.26.3",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-click-outside-modifier": "^2.0.0",
    "ember-concurrency": "^2.1.0",
    "ember-element-helper": "^0.5.5",
    "ember-focus-trap": "^0.7.0",
    "ember-truth-helpers": "^3.0.0",
    "tracked-maps-and-sets": "^3.0.1"
  },
  "devDependencies": {
    "@docfy/ember": "^0.4.4",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.4.0",
    "@embroider/test-setup": "^0.43.5",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@tailwindcss/typography": "^0.4.1",
    "@tailwindcss/ui": "^0.7.2",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/user-event": "^13.1.9",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "dotenv-cli": "^4.0.0",
    "ember-auto-import": "^1.11.2",
    "ember-cli": "~3.27.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deprecation-workflow": "^2.0.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-postcss": "^7.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-css-transitions": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-on-helper": "^0.1.0",
    "ember-page-title": "^6.2.1",
    "ember-popper-modifier": "^1.0.1",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-simple-set-helper": "^0.1.2",
    "ember-source": "~3.28.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.5.1",
    "ember-template-lint-plugin-prettier": "^2.0.1",
    "ember-test-selectors": "^5.0.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-ember": "^10.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.3.2",
    "qunit": "^2.14.1",
    "qunit-dom": "^1.6.0",
    "qunit-wait-for": "^2.0.1",
    "rehype-add-classes": "^1.0.0",
    "release-it": "^14.8.0",
    "release-it-lerna-changelog": "^3.1.0",
    "remark-highlight.js": "^6.0.0",
    "tailwindcss": "^2.2.9",
    "testdouble": "^3.16.1",
    "testdouble-qunit": "^3.0.0"
  },
  "resolutions": {
    "@embroider/core": "^0.43.5",
    "@embroider/macros": "^0.43.5",
    "@embroider/util": "^0.43.5"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.hbs": "ember-template-lint --fix",
    "*.{json,md,yml}": "prettier --write"
  },
  "volta": {
    "node": "14.17.3",
    "yarn": "1.22.10"
  }
}
